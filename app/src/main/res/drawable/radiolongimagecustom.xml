<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <layer-list>
            <item
                android:drawable="@drawable/radiolongunenable"/>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item
                android:drawable="@drawable/radiolongunchecked"/>
        </layer-list>
    </item>
    <item android:state_checked="true">
        <layer-list>
            <item
                android:drawable="@drawable/radiolong"/>
        </layer-list>
    </item>

</selector>